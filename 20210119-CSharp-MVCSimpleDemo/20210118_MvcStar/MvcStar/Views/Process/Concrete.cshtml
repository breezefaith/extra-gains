@model IEnumerable<System.Diagnostics.Process>

@{
    ViewData["Title"] = "Processes using concrete classes";
}

<h1>@ViewData["Title"]</h1>

<table class="table striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Handles</th>
    </tr>
    @foreach(var item in Model) {
        <tr>
            <td>@item.Id </td>
            <td>
                <a asp-controller="Process" class="btn btn-success"
                        asp-action="Display"
                        asp-route-id="@item.Id">@item.ProcessName</a>
            </td>
            <td>@item.HandleCount </td>
        </tr>
    }
</table>