
@{
    ViewData["Title"] = "Processes";
}

<h1>@ViewData["Title"]</h1>

<table class="table striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        @* <th>Handles</th> *@
    </tr>
    @foreach(var item in ViewBag.Processes) {
        <tr>
            <td>@item.Id </td>
            <td>@item.ProcessName </td>
            @* <td>@item.Handles </td> *@
        </tr>
    }
</table>